{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES2022",
    "module": "CommonJS",
    "rootDir": "./",
    "outDir": "./dist",
    "newLine": "lf",
    "noEmitOnError": true,
    "esModuleInterop": true,
    "strictPropertyInitialization": false,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "allowJs": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "types": ["node"]
  }
}
